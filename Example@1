public class exampleone{
 public static void main  (String[]args){  
   int b=5;
   int a = 10;
   int sum  = a+b;
   int difference = a-b;
   int product = a*b;
   
 System.out.println("sum is:" +sum);
 System.out.println("difference is:" +difference); 
 System.out.println("product is:" +product); 
 
   }  
 }
